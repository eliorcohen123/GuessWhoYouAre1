<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.elior.guesswhoyouare"
    android:versionCode="2"
    android:versionName="1.0.0">

    <uses-feature
        android:name="android.hardware.telephony"
        android:required="false" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.VIBRATE" />

    <uses-feature android:name="android.hardware.camera" />

    <application
        android:name=".OtherPackage.NearByApplication"
        android:allowBackup="true"
        android:icon="@drawable/guessicon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/guessicon"
        android:supportsRtl="false"
        android:theme="@style/AppTheme">
        <activity android:name=".ClassesPackage.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ClassesPackage.FavoritesFace" />
        <activity android:name=".ClassesPackage.AddFace" />
        <activity android:name=".ClassesPackage.DeleteFace" />
        <activity android:name=".ClassesPackage.DeleteAllDataFavorites" />

        <service android:name=".OtherPackage.MyFirebaseMessagingService">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>
    </application>

</manifest>